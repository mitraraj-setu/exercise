<odoo>

    <record id="setu_sale_order_import_wizard" model="ir.ui.view">
        <field name="name">setu.sale.order.import.wizard.form</field>
        <field name="model">setu.sale.order.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Sales Orders">
                <group>
                    <field name="file" filename="file_name"/>
                </group>
                <footer>
                    <button name="action_import" type="object" string="Import" class="btn-primary"/>
                    <button name="action_cancel" type="object" string="Cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>